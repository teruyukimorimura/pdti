    <licenses>
        <license>
            <name>Apache 2.0 License</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0</url>
            <distribution>manual</distribution>
        </license>
    </licenses>
    
    <mailingLists>
        <mailingList>
            <name>Provider Directories Test Implementation</name>
            <subscribe>provider-directories-test-implementation+subscribe@googlegroups.com</subscribe>
            <unsubscribe>provider-directories-test-implementation+unsubscribe@googlegroups.com</unsubscribe>
            <post>provider-directories-test-implementation@googlegroups.com</post>
            <archive>https://groups.google.com/forum/#!forum/provider-directories-test-implementation</archive>
        </mailingList>
        <mailingList>
            <name>Provider Directories Test Implementation CI Alerts</name>
            <subscribe>provider-directories-test-implementation-ci-alerts+subscribe@googlegroups.com</subscribe>
            <unsubscribe>provider-directories-test-implementation-ci-alerts+unsubscribe@googlegroups.com</unsubscribe>
