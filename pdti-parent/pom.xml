            </dependency>
            <dependency>
                <groupId>org.apache.directory.api</groupId>
                <artifactId>api-dsml-engine</artifactId>
                <version>1.0.0-M20</version>
                <exclusions>
                    <exclusion>
                        <groupId>org.apache.directory.api</groupId>
                        <artifactId>api-ldap-schema-data</artifactId>
