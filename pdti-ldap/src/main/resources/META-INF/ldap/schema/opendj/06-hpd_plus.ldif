  SINGLE-VALUE
 )
objectClasses: ( 1.3.6.1.4.1.19376.1.2.4.1 
  NAME 'HPDProvider'
  SUP top
  AUXILIARY
  MAY ( hpdProviderStatus $ hpdProviderLanguageSupported $ hpdProviderBillingAddress $ hpdProviderMailingAddress $ hpdProviderPracticeAddress $ hpdMedicalRecordsDeliveryEmailAddress $ memberOf $ hpdProviderLegalAddress $ hpdHasAService $ hpdCredential )
 )
objectClasses: ( 1.3.6.1.4.1.19376.1.2.4.5 
  NAME 'HPDElectronicService'
  SUP top
  STRUCTURAL
  MUST ( hpdServiceAddress $ hpdServiceId )
  MAY ( hpdCertificate $ hpdContentProfile $ hpdIntegrationProfile $ hpdSecurityProfile )
 )
objectClasses: ( 1.3.6.1.4.1.19376.1.2.4.2 
  NAME 'HPDProviderCredential'
  SUP top
  STRUCTURAL
  MUST ( credentialName $ credentialNumber $ credentialType $ hpdCredentialId )
  MAY ( credentialIssueDate $ credentialRenewalDate $ credentialStatus $ credentialDescription $ hpdHasAnOrg $ hpdHasAProvider )
 )
objectClasses: ( 1.3.6.1.4.1.1466.115.121.1.1.7 
  NAME 'hpdMailPerson'
  DESC 'object class holding the hpdRejectMailFrom attribute'
  AUXILIARY
  MUST hpdRejectMailFrom
 )
objectClasses: ( 1.3.6.1.4.1.19376.1.2.4.4 
  NAME 'HPDProviderMembership'
  SUP top
  STRUCTURAL
  MUST ( hpdMemberId $ hpdHasAProvider $ hpdHasAnOrg )
  MAY ( hpdHasAService $ telephoneNumber $ facsimileTelephoneNumber $ mobile $ Pager $ mail )
 )
